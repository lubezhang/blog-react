    "url-loader": "^0.5.6",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.9.0"
  },
  "scripts": {
    "watch-html": "nodemon --watch index.html --exec 'echo `cp index.html build/index.html`'",
    "watch-css": "catw static/css/*.css -o build/css/app.css -v",
    "watch-img": "nodemon --exec 'cp -R static/img/*.* build/img'",
    "watch-js": "npm run build",
    "watch": "npm run watch-css & npm run watch-html & npm run watch-img & npm run serve",
    "clean": "rimraf build && mkdirp build build/css build/img",
    "build": "npm run clean && webpack --config webpack.config.js",
    "serve": "node server.js",
    "api": "nodemon ./server/app.js",
    "start": "npm run build && npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
